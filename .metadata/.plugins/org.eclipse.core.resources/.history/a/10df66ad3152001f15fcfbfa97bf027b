package com.techlabs.model;

public class Board {
	private char board[][];
	private char currentplayer;

	public Board() {
		board = new char[3][3];
		currentplayer = 'X';
		for (int i = 0; i < board.length; i++) {
			for (int j = 0; j < board.length; j++) {
				board[i][j]=' ';
			}
		}

	}

	public boolean iscellempty(int row , int col) {
		board[row][col] =' ';
		return true;

	}

	public void placemark() {

	}

}
