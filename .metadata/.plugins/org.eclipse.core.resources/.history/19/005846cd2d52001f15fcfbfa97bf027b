package com.techlabs.structural.Facade.model;

public class Hotelreception {
	
		
	public Ihotel getIndianmenu(String string) {
		Hotelreception waiter = new Hotelreception();
		 return waiter.getIndianmenu("piiza");
	}

	public IMenu getItalianMenu() {
		Hotelreception waiter1 = new Hotelreception();
		return waiter1.getItalianMenu();

	}
}
