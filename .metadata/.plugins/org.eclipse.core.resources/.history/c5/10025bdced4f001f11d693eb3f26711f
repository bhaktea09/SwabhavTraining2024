package com.techlabs.structural.adapter.model;

public class Biscuit implements Iitems{
	private String name;
	private double mrp;

	public Biscuit(String name, double mrp) {
		super();
		this.name = name;
		this.mrp = mrp;
	}

	@Override
	public String getItemname() {
		// TODO Auto-generated method stub
		return name;
	}

	@Override
	public int getItemPrice() {
		// TODO Auto-generated method stub
		return price;
	}

}
