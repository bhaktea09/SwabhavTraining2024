package com.techlabs.creational.builder.model;

public class Car {
	private Body body;
	private Engine engine;
	private Wheels  wheels;
	public Car(Body body, Engine engine, Wheels wheels) {
		super();
		this.body = body;
		this.engine = engine;
		this.wheels = wheels;
	}
	public Body getBody() {
		return body;
	}
	public void setBody(Body body) {
		this.body = body;
	}
	public Engine getEngine() {
		return engine;
	}
	public void setEngine(Engine engine) {
		this.engine = engine;
	}
	public Wheels getWheels() {
		return wheels;
	}
	public void setWheels(Wheels wheels) {
		this.wheels = wheels;
	}
	public void specifications() {
		System.out.println("body" + body.getShape() +"engine" + engine.getHorsepower() +"wheels" + wheels.getSize() );
	}
}
